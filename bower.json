{
  "name": "gulp-sass-boilerplate",
  "description": "Gulp setup for static websites",
  "main": "index.js",
  "authors": [
    "Ziggy Verstrepen"
  ],
  "license": "ISC",
  "homepage": "",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "jquery": "^3.3.1",
    "normalize-css": "^8.0.0",
    "font-awesome": "^5.0.13"
  },
  "devDependencies": {
    "reset-css": "^4.0.1",
    "animate.css": "^3.5.2"
  },
  "overrides": {
    "font-awesome": {
      "main": [
        "scss/fontawesome.scss",
        "scss/regular.scss",
        "scss/solid.scss",
        "scss/brands.scss",
        "webfonts/*"
      ]
    },
    "normalize-css": {
      "main": [
        "normalize.css"
      ]
    }
  }
}
